---
import Layout from '../layouts/Layout.astro';
const NASA_API_KEY = import.meta.env.NASA_API_KEY;

type Image = {
  title: string;
  explanation: string;
  media_type: 'image' | 'video';
  /** Date published in YYYY-MM-DD format */
  date: string;
  /** Video URL or standard resolution image URL */
  url: string;
  /** High-res image URL (for media_type: 'image' only) */
  hdurl?: string;
  /** API version. Defaults to "v1" */
  service_version: string;
}

const images = await fetch(
  `https://api.nasa.gov/planetary/apod?api_key=${NASA_API_KEY}&count=10`
).then(res => res.json());

console.log(images)
